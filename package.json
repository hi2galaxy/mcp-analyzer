{
  "name": "mcp-analyzer",
  "version": "0.1.0",
  "description": "MCP server for analyzing, planning, and enhancing projects with MCP tools",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-analyzer": "dist/index.js"
  },
  "files": [
    "dist",
    "cursor-mcp.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/index.js",
    "start": "node dist/index.js",
    "dev": "mcp dev npx ts-node-esm src/index.ts",
    "prepare": "npm run build",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --config jest.config.js",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --config jest.config.js --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.1",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcp",
    "cursor",
    "project",
    "analysis",
    "tools"
  ],
  "author": "Jin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hi2galaxy/mcp-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/hi2galaxy/mcp-analyzer/issues"
  },
  "homepage": "https://github.com/hi2galaxy/mcp-analyzer#readme"
}